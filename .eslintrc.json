{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "eqeqeq": "off",
    "curly": "warn",
    "quotes": ["warn", "double"],
    "linebreak-style": ["error", "windows"],
    "no-console": "off"
  }
}
